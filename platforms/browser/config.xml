<?xml version='1.0' encoding='utf-8'?>
<widget id="com.eniacgt.ionic.xjobworker" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>xJobs Worker</name>
    <description>Job Exchange Workers</description>
    <author email="dalvarado@eniacgt.com" href="https://www.eniacgt.com/">Eniac Guatemala</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyDfI8fbeuGwtgTJxEMABzDwHZ8GTH11Tiw" />
    <preference name="GOOGLE_MAPS_IOS_API_KEY" value="AIzaSyDfI8fbeuGwtgTJxEMABzDwHZ8GTH11Tiw" />
    <preference name="Orientation" value="portrait" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
